{
	"reporters": [
        "spec",
        "result",
        "stats",
        "html"
    ],
	"glyphs": {
		"landing": {
			"margin": "━",
			"lane": "⋅",
			"plane": "✈"
		},
		"result": {
			"error": "✖"
		},
		"progress": {
			"empty": "░",
			"fill": "▓"
		},
		"specSteps": {
			"testEnd": "└",
			"step": "│",
			"testBegin": "┌"
		},
		"dotMatrix": {
			"success": ".",
			"failure": "!",
			"unknown": "?",
			"pending": "-"
		},
		"spec": {
			"ok": "✓",
			"pending": "-"
		}
	},
	"testDiscovery": [
		"trial.discovery.spec.SpecTestDiscovery"
	],
	"runInParallel": false,
	"maxThreads": 1
}
